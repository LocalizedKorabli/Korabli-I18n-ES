msgid "IDS_DOGTAG_PCNB071_DESCRIPTION"
msgstr "Para los participantes de «Space Warships 2018»."

msgid "IDS_DOGTAG_PCNB071_NAME"
msgstr "«Space Warships 2018»"

msgid "IDS_DOGTAG_PCNB072_DESCRIPTION"
msgstr "Para los participantes de «Space Warships 2018»."

msgid "IDS_DOGTAG_PCNB072_NAME"
