msgstr "Departamento de Armamento del Instituto Marítimo femenino de Yokosuka"

msgid "IDS_PCEE175_WEAPONS_DEPARTMENT_FLAG_DESCRIPTION"
msgstr ""
"Emblema del Departamento de Armamento del Instituto Marítimo femenino de "
"Yokosuka."

msgid "IDS_PCEE175_WEAPONS_DEPARTMENT_FLAG_EXTRA_DESCRIPTION"
msgstr "De la serie «High School Fleet»."

msgid "IDS_PCEE176_NAVIGATION_DEPARTMENT_FLAG"
msgstr ""
"Departamento de Navegación del Instituto Marítimo femenino de Yokosuka"

msgid "IDS_PCEE176_NAVIGATION_DEPARTMENT_FLAG_DESCRIPTION"
msgstr ""
"Emblema del Departamento de Navegación del Instituto Marítimo femenino de "
"Yokosuka.."

msgid "IDS_PCEE176_NAVIGATION_DEPARTMENT_FLAG_EXTRA_DESCRIPTION"
msgstr "De la serie «High School Fleet»."

msgid "IDS_PCEE177_ENGINEERING_DEPARTMENT_FLAG"
msgstr ""
"Departamento de Ingeniería del Instituto Marítimo femenino de Yokosuka"

msgid "IDS_PCEE177_ENGINEERING_DEPARTMENT_FLAG_DESCRIPTION"
msgstr ""
"Emblema del Departamento de Ingeniería del Instituto Marítimo femenino de "
